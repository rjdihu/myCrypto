nts/CryptoCard.js","original":{"line":72,"column":18}},{"generated":{"line":113,"column":8},"source":"components/CryptoCard.js","original":{"line":72,"column":18}},{"generated":{"line":113,"column":17},"source":"components/CryptoCard.js","original":{"line":72,"column":27}},{"generated":{"line":114,"column":0},"source":"components/CryptoCard.js","original":{"line":72,"column":12}},{"generated":{"line":114,"column":8},"source":"components/CryptoCard.js","original":{"line":70,"column":10}},{"generated":{"line":114,"column":9},"source":"components/CryptoCard.js","original":{"line":65,"column":8}},{"generated":{"line":114,"column":10},"source":"components/CryptoCard.js","original":{"line":53,"column":6}},{"generated":{"line":114,"column":11},"source":"components/CryptoCard.js","original":{"line":52,"column":4}},{"generated":{"line":115,"column":0},"source":"components/CryptoCard.js","original":{"line":77,"column":3}},{"name":"React","generated":{"line":119,"column":2},"source":"components/CryptoCard.js","original":{"line":4,"column":25}},{"name":"Component","generated":{"line":119,"column":17},"source":"components/CryptoCard.js","original":{"line":4,"column":31}},{"name":"CryptoCard","generated":{"line":121,"column":15},"source":"components/CryptoCard.js","original":{"line":80,"column":15}}],"sources":{"components/CryptoCard.js":"import React from 'react'\nimport bitcoin from '../assets/bitcoin.png'\n\nclass CryptoCard extends React.Component {\n  constructor(props) {\n    super(props)\n    this.state = {\n      name: props.name,\n      symbol: props.symbol,\n      price: null,\n      lastPrice: null,\n    }\n\n    this.pollPrice = this.pollPrice.bind(this)\n  }\n\n  componentDidMount() {\n    this.pollPrice()\n    setInterval(this.pollPrice, 10000)\n  }\n\n  pollPrice() {\n    console.log('polling for new price')\n    const { symbol } = this.state\n    fetch(`https://min-api.cryptocompare.com/data/price?fsym=${symbol}&tsyms=${symbol},USD`)\n      .then(resp => resp.json())\n      .then(json => {\n        this.setState((prevState) => ({\n          price: json.USD,\n          lastPrice: prevState.price !== json.USD\n            ? prevState.price\n            : prevState.lastPrice\n        }))\n      })\n  }\n\n  priceChange(lastPrice, price) {\n    const diff = lastPrice - price\n    const change = diff / lastPrice\n    const percent = -(change * 100)\n    return (change === -Infinity\n      ? 0\n      : percent).toFixed(3)\n  }\n\n  render() {\n    const { name, symbol, price, lastPrice } = this.state\n    const gainloss = lastPrice > price\n      ? 'loss'\n      : 'gain'\n\n    return (\n      <div className={`card ${gainloss}`}>\n        <div className='top'>\n          <div className='name'>\n            {name}\n            <span>({symbol})</span>\n          </div>\n\n          <div className={`percentage ${gainloss}`}>\n            {this.priceChange(lastPrice, price)}%\n          </div>\n        </div>\n\n        <div className='bottom'>\n          <div className='bitcoin'>\n            <img id=\"bitcoin\" src={bitcoin} alt={symbol} />\n          </div>\n\n          <div className={`price ${gainloss}`}>\n            ${price}\n            <span className={`triangle`} />\n          </div>\n        </div>\n      </div>\n    )\n  }\n}\n\nexport default CryptoCard\n"},"lineCount":null}},"hash":"bda051cad761e1d7731a753d5fa1ce52","cacheData":{"env":{}}}